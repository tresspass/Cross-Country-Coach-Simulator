package coachsimulator;

import java.awt.Container;

//Main game starter
public class Main {

	public static void main(String[] args){
		//starting window
		Window w = new Window();
		w.Init();
		w.setVisible(true);
		boolean isInit = true;
		//Init game
		while(isInit){
			GameIntro gi = new GameIntro();
		}
		//Starting Loop
		Loop gl = new Loop();
		gl.runGameLoop();
	}
	
}
