package coachsimulator;

import java.awt.Container;

//Main game starter
public class Main {

	public static void main(String[] args){
		//starting window
		Window w = new Window();
		w.setVisible(true);
		//Init game
		if(w.isVisible()){
			//Starting Loop
				if(Loop.running){
				Loop gl = new Loop();
				gl.runGameLoop();
		}
	}
	
}
